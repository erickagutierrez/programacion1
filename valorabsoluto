#include<iostream>
#include"stdafx.h"
#include"conio.h"
using namespace std;


int main()
{
	float a;
	cout << "Ingrese un numero: " << endl;
	cin >> a;
	if (a<0) 
		a = -a;
	cout << "El valor absoluto es: " << a << endl;
	getch();
	return 0;
}

